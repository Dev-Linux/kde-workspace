<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.NotificationAreaWatcher">

    <method name="RegisterService">
       <arg name="service" type="s" direction="in"/>
    </method>

    <method name="RegisteredServices">
        <arg type="as" direction="out"/>
        <annotation name="com.trolltech.QtDBus.QtTypeName.Out0" value="QStringList"/>
    </method>


    <signal name="ServiceRegistered">
        <arg type="s"/>
    </signal>

    <signal name="ServiceUnregistered">
        <arg type="s"/>
    </signal>

    <method name="RegisterNotificationArea">
       <arg name="service" type="s" direction="in"/>
    </method>

    <method name="IsNotificationAreaRegistered">
       <arg name="service" type="b" direction="out"/>
    </method>

    <signal name="NotificationAreaRegistered">
    </signal>
  </interface>
</node>
