	-rm -f  customize.moc.cpp main.moc.cpp general.moc.cpp

#>+ 8
libkcm_drkonqi.la.closure: $(libkcm_drkonqi_la_OBJECTS) $(libkcm_drkonqi_la_DEPENDENCIES)
	@echo "int main() {}" > libkcm_drkonqi_la_closure.cpp
	@$(LTCXXCOMPILE) -c libkcm_drkonqi_la_closure.cpp
	@$(CXXLINK) libkcm_drkonqi_la_closure.lo $(libkcm_drkonqi_la_LDFLAGS) $(libkcm_drkonqi_la_OBJECTS) $(libkcm_drkonqi_la_LIBADD) $(LIBS)
	@rm -f libkcm_drkonqi_la_closure.* libkcm_drkonqi.la.closure
	@echo "timestamp" > libkcm_drkonqi.la.closure


