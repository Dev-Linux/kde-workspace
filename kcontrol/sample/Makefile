#>+ 5
force-reedit:
		cd $(top_srcdir) && $(AUTOMAKE) --gnu kcontrol/drkonqi/Makefile
	cd $(top_srcdir) && perl admin/am_edit kcontrol/drkonqi/Makefile.in


#>+ 11
libkcm_drkonqi_la.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/main.cpp $(srcdir)/general.cpp $(srcdir)/customize.cpp  customize.moc.cpp main.moc.cpp general.moc.cpp
	@echo 'creating libkcm_drkonqi_la.all_cpp.cpp ...'; \
	rm -f libkcm_drkonqi_la.all_cpp.files libkcm_drkonqi_la.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> libkcm_drkonqi_la.all_cpp.final; \
	for file in main.cpp general.cpp customize.cpp  customize.moc.cpp main.moc.cpp general.moc.cpp; do \
	  echo "#include \"$$file\"" >> libkcm_drkonqi_la.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> libkcm_drkonqi_la.all_cpp.final; \
	done; \
	cat libkcm_drkonqi_la.all_cpp.final libkcm_drkonqi_la.all_cpp.files  > libkcm_drkonqi_la.all_cpp.cpp; \
	rm -f libkcm_drkonqi_la.all_cpp.final libkcm_drkonqi_la.all_cpp.files

#>+ 3
clean-final:
	-rm -f libkcm_drkonqi_la.all_cpp.cpp

#>+ 2
final:
	$(MAKE) libkcm_drkonqi_la_OBJECTS="$(libkcm_drkonqi_la_final_OBJECTS)" all-am
