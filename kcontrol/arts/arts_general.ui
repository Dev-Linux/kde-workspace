                    </widget>
                <widget class="QCheckBox" row="3" column="0" rowspan="1" colspan="3">
                    <property name="name">
                        <cstring>startRealtime</cstring>
                    </property>
                    <property name="text">
                        <string>Run soundserver with &amp;realtime priority</string>
                    </property>
                    <property name="whatsThis" stdset="0">
