lib_LTLIBRARIES = libkcm_smserver.la

libkcm_smserver_la_SOURCES = kcmsmserver.cpp
libkcm_smserver_la_LDFLAGS  = $(all_libraries) -module -avoid-version -no-undefined
libkcm_smserver_la_LIBADD = -lkdeui
INCLUDES= $(all_includes) 

libkcm_smserver_la_METASOURCES = AUTO

noinst_HEADERS = kcmsmserver.h

KDE_ICON = go

messages:
	$(XGETTEXT) $(libkcm_smserver_la_SOURCES) -o $(podir)/kcmsmserver.pot

data_DATA = kcmsmserver.desktop
datadir = $(kde_appsdir)/Settings/LookNFeel/

EXTRA_DIST = $(data_DATA)
