<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>KWidgetSettingsModule</class>
<author>Daniel Molkentin &lt;molkentin@kde.org&gt;</author>
<widget class="KCModule">
    <property name="name">
        <cstring>KWidgetSettingsModule</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>534</width>
            <height>407</height>
        </rect>
    </property>
    <property name="caption">
        <string>Widget Settings</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>gbMiscToolbarSet</cstring>
            </property>
            <property name="frameShape">
                <enum>Box</enum>
            </property>
            <property name="frameShadow">
                <enum>Sunken</enum>
            </property>
            <property name="title">
                <string>Misc Toolbar Settings</string>
            </property>
            <property name="toolTip" stdset="0">
                <string></string>
            </property>
            <property name="whatsThis" stdset="0">
                <string>&lt;b&gt;Note:&lt;/b&gt; that all widgets in this combobox do not apply to Qt-only applications!</string>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QCheckBox" row="0" column="0" rowspan="1" colspan="3">
                    <property name="name">
                        <cstring>cboxHoverButtons</cstring>
                    </property>
                    <property name="text">
                        <string>High&amp;light buttons under mouse</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>If this option is selected, toolbar buttons will change their color when the mouse cursor is moved over them.</string>
                    </property>
                </widget>
                <spacer row="3" column="2">
                    <property name="name" stdset="0">
                        <cstring>Spacer2</cstring>
                    </property>
                    <property name="orientation">
                        <enum>Horizontal</enum>
                    </property>
                    <property name="sizeType">
                        <enum>Expanding</enum>
                    </property>
                    <property name="sizeHint">
                        <size>
                            <width>20</width>
                            <height>20</height>
                        </size>
                    </property>
                </spacer>
                <widget class="QComboBox" row="3" column="1">
                    <item>
                        <property name="text">
                            <string>Icons only</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Text only</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Text aside icons</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Text under icons</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>cbToolbarIcons</cstring>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>&lt;p&gt;&lt;b&gt;Icons only:&lt;/b&gt; Shows only icons on toolbar buttons. Best option for low resolutions.&lt;/p&gt;&lt;p&gt;&lt;b&gt;Text only: &lt;/b&gt;Shows only text on toolbar buttons.&lt;/p&gt;&lt;p&gt;&lt;b&gt;Text aside icons: &lt;/b&gt; Shows icons and text on toolbar buttons. Text is aligned aside the icon.&lt;/p&gt;&lt;p&gt;&lt;b&gt;Text under icons: &lt;/b&gt; Shows icons and text on toolbar buttons. Text is aligned below the icon.&lt;/p&gt;</string>
                    </property>
                </widget>
                <widget class="QLabel" row="3" column="0">
                    <property name="name">
                        <cstring>txtToolbarIcons</cstring>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>4</hsizetype>
                            <vsizetype>5</vsizetype>
                            <horstretch>0</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="text">
                        <string>Toolbar &amp;Icons:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>cbToolbarIcons</cstring>
                    </property>
                </widget>
                <widget class="QCheckBox" row="1" column="0" rowspan="1" colspan="3">
                    <property name="name">
                        <cstring>cboxToolbarsHighlight</cstring>
                    </property>
                    <property name="text">
                        <string>Transparent tool&amp;bars when moving</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>If you check this box, the toolbars will be transparent when moving them around.</string>
                    </property>
                </widget>
                <widget class="QCheckBox" row="2" column="0" rowspan="1" colspan="3">
                    <property name="name">
                        <cstring>cboxEnableTooltips</cstring>
                    </property>
                    <property name="text">
                        <string>E&amp;nable Tooltips</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string></string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>If you check this option, the KDE application will offer tooltips when the cursor remains over items in the toolbar.</string>
                    </property>
                </widget>
            </grid>
        </widget>
        <widget class="QCheckBox">
            <property name="name">
                <cstring>cboxEnableGUIEffects</cstring>
            </property>
            <property name="text">
                <string>&amp;Enable GUI Effects</string>
            </property>
            <property name="whatsThis" stdset="0">
                <string>If you check this box, you can select several effects for different widgets like combo boxes or tooltips.</string>
            </property>
        </widget>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>gbGuiEffect</cstring>
            </property>
            <property name="enabled">
                <bool>false</bool>
            </property>
            <property name="title">
                <string>GUI Effects</string>
            </property>
            <property name="whatsThis" stdset="0">
                <string>&lt;p&gt;&lt;b&gt;Disable: &lt;/b&gt;Don't use an effect on this widget&lt;/p&gt;
&lt;p&gt;&lt;b&gt;Animate: &lt;/b&gt;Do some animation&lt;/p&gt;
&lt;p&gt;&lt;b&gt;Fade: &lt;/b&gt;Fade in the widget using alpha-blending&lt;/p&gt;</string>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QLabel" row="2" column="0">
                    <property name="name">
                        <cstring>TextLabel4</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;ToolTip Effect:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>cbTooltipEffect</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="0" column="0">
                    <property name="name">
                        <cstring>TextLabel2</cstring>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>4</hsizetype>
                            <vsizetype>1</vsizetype>
                            <horstretch>0</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="text">
                        <string>&amp;Menu Effect:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>cbMenuEffect</cstring>
                    </property>
                </widget>
                <widget class="QComboBox" row="2" column="1">
                    <item>
                        <property name="text">
                            <string>Disable</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Animate</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Fade</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>cbTooltipEffect</cstring>
                    </property>
                </widget>
                <widget class="QComboBox" row="0" column="1">
                    <item>
                        <property name="text">
                            <string>Disable</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Animate</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Fade</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>cbMenuEffect</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="1" column="0">
                    <property name="name">
                        <cstring>TextLabel3</cstring>
                    </property>
                    <property name="text">
                        <string>ComboBo&amp;x Effect:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>cbComboEffect</cstring>
                    </property>
                </widget>
                <widget class="QComboBox" row="1" column="1">
                    <item>
                        <property name="text">
                            <string>Disable</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Animate</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>cbComboEffect</cstring>
                    </property>
                </widget>
            </grid>
        </widget>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>gbVisual</cstring>
            </property>
            <property name="title">
                <string>Visual Appearance</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QCheckBox">
                    <property name="name">
                        <cstring>cboxIconSupport</cstring>
                    </property>
                    <property name="text">
                        <string>Sho&amp;w icons on buttons</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string></string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>If you enable this option, KDE Applications will show small icons aside of some important buttons.</string>
                    </property>
                </widget>
                <widget class="QCheckBox">
                    <property name="name">
                        <cstring>cboxTearOffHandles</cstring>
                    </property>
                    <property name="text">
                        <string>Show tear-off handles in &amp;pop-up menus</string>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string></string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>If you enable this option some pop-up menus will show so called tear-off handles. If you click them, you get the menu inside a widget. This can be very helpful when performing the same action multiple times.</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <spacer>
            <property name="name" stdset="0">
                <cstring>Spacer3</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>20</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>cboxEnableGUIEffects</sender>
        <signal>toggled(bool)</signal>
        <receiver>gbGuiEffect</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>cboxHoverButtons</tabstop>
    <tabstop>cboxToolbarsHighlight</tabstop>
    <tabstop>cboxEnableTooltips</tabstop>
    <tabstop>cbToolbarIcons</tabstop>
    <tabstop>cboxEnableGUIEffects</tabstop>
    <tabstop>cbMenuEffect</tabstop>
    <tabstop>cbComboEffect</tabstop>
    <tabstop>cbTooltipEffect</tabstop>
    <tabstop>cboxIconSupport</tabstop>
    <tabstop>cboxTearOffHandles</tabstop>
</tabstops>
<includes>
    <include location="global" impldecl="in declaration">kcmodule.h</include>
    <include location="local" impldecl="in implementation">widgetsettings.ui.h</include>
</includes>
<slots>
    <slot>init()</slot>
    <slot specifier="non virtual">setDirty()</slot>
    <slot>defaults()</slot>
    <slot>load()</slot>
    <slot>save()</slot>
    <slot returnType="const KAboutData *">aboutData() const</slot>
    <slot returnType="QString">quickHelp() const</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
