	rm -f libkcm_knotify_la.all_cpp.files libkcm_knotify_la.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> libkcm_knotify_la.all_cpp.final; \
	for file in main.cpp knotify.cpp ; do \
	  echo "#include \"$$file\"" >> libkcm_knotify_la.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> libkcm_knotify_la.all_cpp.final; \
	done; \
	cat libkcm_knotify_la.all_cpp.final libkcm_knotify_la.all_cpp.files  > libkcm_knotify_la.all_cpp.cpp; \
	rm -f libkcm_knotify_la.all_cpp.final libkcm_knotify_la.all_cpp.files

#>+ 3
clean-final:
	-rm -f libkcm_knotify_la.all_cpp.cpp

#>+ 2
final:
	$(MAKE) libkcm_knotify_la_OBJECTS="$(libkcm_knotify_la_final_OBJECTS)" all-am
#>+ 3
cvs-clean:
	$(MAKE) -f $(top_srcdir)/admin/Makefile.common cvs-clean

#>+ 3
kde-rpo-clean:
	-rm -f *.rpo
